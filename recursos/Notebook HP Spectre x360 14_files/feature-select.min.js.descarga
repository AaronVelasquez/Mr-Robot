define(['jquery','uiComponent',],function($,Component){return Component.extend({initialize:function(config){var related=config.related;if(related){related=JSON.stringify(related);var relatedObj=eval('['+related+']');related=(relatedObj&&relatedObj[0])?relatedObj[0]:null;}
$('#config-select').change(function(){window.location.href=$(this).children('option:selected').data('url');});$('.product-view-choose-color .list').click(function(){if(!related){return false;}
var selectColor=$(this).data('label');var url=$(this).children('a').data('url');var selectColorSelected=$(this).hasClass('selected');if(related[selectColor]){if(!selectColorSelected){$('.product-view-choose-color .list').each(function(k,v){$(v).removeClass('selected');});$(this).addClass('selected');$('.product-view-choose-color').removeClass('please-select');$.ajaxSetup({showLoader:true});$.post(url,{'isAjax':true});window.location.href=url;}}});},});});