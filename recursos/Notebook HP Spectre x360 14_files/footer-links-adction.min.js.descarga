define(['jquery','uiComponent',],function($,Component){return Component.extend({initialize:function(){this._bindEvents();},_bindEvents:function(){$('.footer.content .footer-additional-info .footer-additional-info-left .block-title').bind('click',function(){var maxHeight=$(this).next().height();if(maxHeight>0){$(this).parent().removeClass('active');}else{$(this).parent().addClass('active');}});}});});