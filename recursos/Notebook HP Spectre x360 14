        var isProductAvailableForPickup = false,
            pincodeUrls = {clickAndCollectCheckUrl: "https://www.hp.com/cl-es/shop/clickandcollect/index/check/"},
            checkEddUrl = 'https://www.hp.com/cl-es/shop/rest/V1/edd/pdp/',
            cascadingAddressVisible = true,
            isConfigurableProduct = Boolean(
                            ),
            payload = {
                'productId': '10045',
                'sku': '6B9Q1LA'
            },
            pincodeTrans = {invalidCode: "Ingrese un código postal válido"};
    </script>

