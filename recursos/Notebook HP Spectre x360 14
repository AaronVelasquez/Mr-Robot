<script>
    require(['jquery', 'swipeSlider','stellarGlobal', 'mage/translate'], function ($, Swiper, stellarGlobal, $t) {
        var stellarGlobal = new stellarGlobal(), countryArray = ['MX', 'CO', 'CL', 'PE', 'AR', "BR"];

        if($(window).width()<=768){
            var item = $('.selected-products .package2 .item');
            item.each(function (){
                $(this).find('.bg').height($(this).width());
            });
            $(window).resize(function (){
                var item = $('.selected-products .package2 .item');
                item.each(function (){
                    $(this).find('.bg').height($(this).width());
                });
            });
            if (!countryArray.includes(window.countryId)){
                stellarGlobal.slideShow('#product-related-links .form-title', true, '.selected-products');
            }
        }
        stellarGlobal.swiperItem('.product.info.detailed .recommended-products', 16, 5.5);
        stellarGlobal.swiperItem('.product.info.detailed .other-recommend-product', 16, 5.5);
    })
</script>
