                    <select disabled="" id="district_id" name="district_id" title="Localidad" class="validate-select" data-validate="{required:true}">
                        <option value="">Seleccione una localidad.</option>
                    </select>
                    <input type="text" name="district" value="" title="Localidad" class="input-text hide " id="district">
                </div>
