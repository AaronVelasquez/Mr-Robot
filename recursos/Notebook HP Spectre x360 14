        <% if(data.purchase_message) { %>
        <div class="purchase-message" style="display:none;"><%- data.purchase_message %></div>
        <% } %>
    </div>
</script>


<script>
    var isEnableNewVersionInPopup = 1;
    require([
        'jquery',
        'mage/translate',
        'HPOLS_Catalog/js/popup',
        'mage/template',
        'Magento_Catalog/js/price-utils',
        'swiperSlide7',
        'stellarGlobal',
        'renderLazyLoad'
    ], function ($, $t, $p, $temp, priceUtils, Swiper, stellarGlobal, renderLazyLoad) {
        $('#new-popup-recommended-products .product.actions.product-item-actions > label').click(function () {
            $(this).next().trigger('click');
        });
        function recommendedSwiper (container, slideLength, slidePerView, pagination, prevEL, nextEl) {
                if (slideLength > 0) {
                    var screenWidth = $(window).width(),
                        slidesPerView = 4;
                    if (screenWidth < 768) {
                        slidesPerView = 2;
                    }
                    var swipeConfig = {
                        spaceBetween: 16,
                        observer:true,
                        observeSlideChildren:true,
                        pagination: {
                            el: pagination,
                            clickable: true

                        },
                        navigation: {
                            prevEl: prevEL,
                            nextEl: nextEl
                        },
                        breakpoints: {
                            320: {
                                slidesPerView: 1,
                                slidesPerGroup: 1
                            },
                            360: {
                                slidesPerView: 1.2,
                                slidesPerGroup: 1
                            },
                            580: {
                                slidesPerView: 2.2,
                                slidesPerGroup: 2
                            },
                            641: {
                                slidesPerView: 2.5,
                                slidesPerGroup: 2
                            },
                            769: {
                                slidesPerView: 3.2,
                                slidesPerGroup: 3
                            },
                            1025: {
                                slidesPerView: 3.5,
                                slidesPerGroup: 3
                            },
                            1280: {
                                slidesPerView: 3.5,
                                slidesPerGroup: 3
                            },
                            1367: {
                                slidesPerView: slidePerView,
                                slidesPerGroup: slidePerView - .5
                            }
                        },
                        on: {
                            slideChangeTransitionEnd: function(swiper,event){
                                $('.swiper-page-number').find('.current').text(this.activeIndex+1);
                            },
                            paginationRender: function (){
                                var pageNumber = "<div class='swiper-page-number'><span class='current'>"+
                                    (this.activeIndex+1)+"</span>/<span class='total'>"+
                                    this.$wrapperEl[0].childElementCount+"</span></div>";
                                if(!$(container+' .swiper-page-number').length){
                                    $(container).find('.dynamic-pagination').prepend(pageNumber);
                                } else {
                                    $(container+' .swiper-page-number').find('.current').text((this.activeIndex+1));
                                }
                                if (this.pagination.bullets.length === 1) {
                                    $(container).siblings('.swiper-button-prev').hide();
                                    $(container).siblings('.swiper-button-next').hide();

                                } else {
                                    $(container).siblings('.swiper-button-prev').show();
                                    $(container).siblings('.swiper-button-next').show();
                                }
                            },
                            slideChange: function () {
                                setTimeout(function () {
                                    renderLazyLoad.initLoading('.mplazyload-icon');
                                }, 2000);
                            }
                        },
                    };
                    if($(window).width()<768){
                        swipeConfig.pagination.dynamicBullets = true;
                    }
                    if (slideLength <= slidesPerView) {
                        // $(container).find('.swiper-button-next,.swiper-button-prev').hide()
                        // $('#new-popup-recommended-products .swiper-button-next,.swiper-button-prev').hide()
                    }
                    new Swiper(container, swipeConfig);
                }
        }
            if (isEnableNewVersionInPopup === 1) {
                if ($('.related-title-tabs').find('li').length < 1) {
                    $('.related-title-tabs').hide();
                    $('.mobile-related-title-tabs').hide();
                }
                var slideLength = $('.recommended-products-swiper .swiper-slide').length;
                if (slideLength < 1) {
                    $('.catalog-product-popup .related-title-tabs').hide();
                    $('.catalog-product-popup .mobile-related-title-tabs').hide();
                }
                recommendedSwiper('.catalog-product-popup .recommended-products .swiper', slideLength, 3.5, '.recommended-product-popup .recommended-products .swiper-pagination', '.recommended-product-popup .recommended-products .swiper-button-prev', '.recommended-product-popup .recommended-products .swiper-button-next');

                var recommendedProductsPopup = $('.catalog-product-popup').find('.product-dynamic-discount');
                if (recommendedProductsPopup.length) {
                    if ($('.product-add-form').find('.product-dynamic-discount').hasClass('hide')) {
                        $('.product-add-form').find('.product-dynamic-discount').removeClass('hide');
                        if (!recommendedProductsPopup.hasClass('hide')) {
                            recommendedProductsPopup.addClass('hide');
                        }
                    } else {
                        recommendedProductsPopup.removeClass('hide');
                    }
                }
                var tabsEl = '.recommended-product-popup .related-title-tabs';
                new Swiper(tabsEl + ' .swiper', {
                    slidesPerView: "auto",
                    spaceBetween: 0,
                    observable: true,
                    observeSlideChildren: true,
                    navigation: {
                        prevEl: tabsEl + ' .swiper-button-prev',
                        nextEl: tabsEl + ' .swiper-button-next',
                    },
                    pagination: {
                        el: tabsEl+' .swiper-pagination',
                        type: 'fraction',
                        formatFractionTotal: function (number) {
                            if (number > 1) {
                                $(tabsEl).find('ul').css({
                                    'justify-content': 'flex-start'
                                });
                            } else {
                                $(tabsEl).find('ul').css({
                                    'justify-content': 'center'
                                });
                            }
                        },
                    },
                });
                setTimeout(function () {
                    renderLazyLoad.initLoading('.mplazyload-icon');
                }, 2000);
            }
            addRecommendToCart('.recommended-product-popup label.add-item');
            if ($('.catalog-product-popup').find('.other-recommend-product').length < 1){
                $('.catalog-product-popup').find('.recommended-products').after($('.other-recommend-product').clone(true));
            }
            $('.related-title-tabs').find('li').click(function () {
                if (!$(this).hasClass('.recommend')) {
                    $('.catalog-product-popup').find('.other-recommend-product').eq($(this).attr('data-related-index')).show();
                    $('.catalog-product-popup').find('.other-recommend-product').eq($(this).attr('data-related-index')).siblings('.other-recommend-product').hide();
                    var el = $('.catalog-product-popup').find('.other-recommend-product').eq($(this).attr('data-related-index')).attr('id'),
                        _this = $(this),
                        slideLength = $('#' + el).find('.swiper-slide').length;
                    recommendedSwiper('.recommended-product-popup #' + el + ' .swiper', slideLength, 3.5, '.recommended-product-popup #' + el + ' .swiper-pagination', '.recommended-product-popup #' + el + ' .swiper-button-prev', '.recommended-product-popup #' + el + ' .swiper-button-next');
                    addRecommendToCart('.recommended-product-popup button.related-add-to-bubble');
                }
            })


            function addRecommendToCart(doAction) {
                var slideLength = 0,
                    slidesPerView = 2,
                    container = '.catalog-product-popup .simple-popup-main-content .selected-products-list',
                    $parent = $(container).parents('.selected-products-popup');
                $(doAction).off('click').on('click', function () {
                    var _this = $(this);
                    if (!$(this).hasClass('added-action') && !$(this).hasClass('adding-action')) {
                        $('.catalog-product-popup').find('.swiper').show();
                        $(this).addClass('adding-action').siblings('input').prop('checked', true);
                        $(this).find('span').text($t('Remove Item'));
                        var productDataStr = $(this).closest('.product-item-info').attr('data-product'),
                            productData = JSON.parse(productDataStr),
                            purchaseMessage =
                                $(this).closest('.product-item-details').find('.purchase-by-purchase-message').attr('data-msg');
                        productData['format_price'] = priceUtils.formatPrice(productData.price);
                        productData['purchase_message'] = purchaseMessage;
                        productData['url'] = productData['url'] === '#' ? 'javaScript:;' : productData['url'];
                        var alt = $(this).closest('.product-item-info').find('.product-image-photo').attr('alt');

                        productData['alt'] = alt ? alt : productData['name'];

                        var template = $temp('#dynamic-discount-template');
                        var templateHtml = template({
                            data: productData
                        });
                        slideLength = $(container).find('.added-item').length + 1;
                        $(container).prepend(templateHtml);
                        if ($(window).width() > 768) {
                            if (slideLength > slidesPerView) {
                                productSwiper(slideLength, 2.2, '.selected-products-popup .swiper', '.selected-product-swiper-button-prev', '.selected-product-swiper-button-next');
                                // $parent.find('.selected-product-swiper-button-prev, .selected-product-swiper-button-next').show();
                            } else {
                                // $parent.find('.selected-product-swiper-button-prev, .selected-product-swiper-button-next').hide();
                            }
                        } else {
                            if (slideLength > 1) {
                                productSwiper(slideLength, 2, '.selected-products-popup .swiper', '.selected-product-swiper-button-prev', '.selected-product-swiper-button-next');
                                // $parent.find('.selected-product-swiper-button-prev, .selected-product-swiper-button-prev').show();
                            } else {
                                // $parent.find('.selected-product-swiper-button-prev, .selected-product-swiper-button-prev').hide();
                            }
                        }
                        var productId = _this.parents('.product-item-info').attr('data-product-id');
                        removeSelected('.remove-btn#id-' + productId);
                    } else {
                        var productId = _this.parents('.product-item-info').attr('data-product-id');
                        $('.recommended-product-popup').find('#id-' + productId).trigger('click');

                    }
                    setTimeout(function () {
                        if (_this.hasClass('adding-action')) {
                            _this.removeClass('adding-action').addClass('added-action');
                        }
                    }, 1500);
                    var addedItemCount = $(container).find('.added-item').length;
                    $('.add-to-cart-popup-title').html(
                        $t("%1 ITEMS ADDED <span>TO YOUR BASKET </span>").replace('%1', addedItemCount)
                    );
                    return false;
                });
            }

            function removeSelected(doAction) {
                var el = $('.catalog-product-popup .selected-products-list'),
                    _parent = el.parents('.selected-products-popup');
                $(doAction).off('click').on('click', function () {
                    $('label.' + $(this).attr('id')).siblings('input').prop('checked', false);
                    $('.' + $(this).attr('id')).removeClass('added-action adding-action').find('span').text($t('Add Item'));
                    $(this).closest('.added-item').remove();
                    var slideLength = el.find('.added-item').length;

                    if ($(window).width() > 768) {
                        if (slideLength > 2) {
                            productSwiper(slideLength, 2, '.selected-products-popup .swiper', '.selected-product-swiper-button-prev', '.selected-product-swiper-button-next');
                            // _parent.find('.selected-product-swiper-button-prev, .selected-product-swiper-button-next').show();
                        } else {
                            // _parent.find('.selected-product-swiper-button-prev, .selected-product-swiper-button-next').hide();
                        }
                    } else {
                        if (slideLength > 1) {
                            productSwiper(slideLength, 2, '.selected-products-popup .swiper', '.selected-product-swiper-button-prev', '.selected-product-swiper-button-next');
                            // _parent.find('.selected-product-swiper-button-prev, .selected-product-swiper-button-next').show();
                        } else {
                            // _parent.find('.selected-product-swiper-button-prev, .selected-product-swiper-button-next').hide();
                        }
                    }
                    var addedItemCount = $('.catalog-product-popup._show ' +
                        '.simple-popup-main-content .selected-products-popup').find('.added-item').length;
                    $('.add-to-cart-popup-title').html(
                        $t("%1 ITEMS ADDED <span>TO YOUR BASKET </span>").replace('%1', addedItemCount)
                    );
                    return false;
                })
            }

            function closeMessage(container) {
                $(container).click(function () {
                    $(this).closest('div').remove();
                });
            }

            function getCookie(cookieName) {
                var cookie = " " + document.cookie;
                var search = " " + cookieName + "=";
                var setStr = 'null';
                var offset = 0;
                var end = 0;
                if (cookie.length > 0) {
                    offset = cookie.indexOf(search);
                    if (offset !== -1) {
                        offset += search.length;
                        end = cookie.indexOf(";", offset);
                        if (end === -1) {
                            end = cookie.length;
                        }
                        setStr = unescape(cookie.substring(offset, end));
                    }
                }
                return (setStr);
            }

            function productSwiper(slideLength, slidesPerView, container, prev, next) {
                var swipeConfig = {
                    slidesPerView: slidesPerView,
                    spaceBetween: 16,
                    observable: true,
                    observeSlideChildren: true,
                    // navigation: {
                    //     prevEl: prev,
                    //     nextEl: next,
                    // },

                };
                if (slideLength <= slidesPerView) {
                    // $(slidesPerView).find(' .swiper-button-next, .swiper-button-prev').hide();
                    // $(slidesPerView).find(' .selected-product-swiper-button-next, .selected-product-swiper-button-prev').hide();
                    swipeConfig.simulateTouch = false;
                }

                new Swiper(container, swipeConfig);
            }
    })
</script>
</div>
    

            
                            </div>
                                    </div>
            </div>
        
            
    
    
    
        
    
    </form>
</div>
