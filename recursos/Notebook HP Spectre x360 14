<script>
    require(['jquery', 'contactUs'], function($) {
        const container = '.contact-us-container';
        const initPlugin = function(ref) {
            $(ref).find('.live-chat').on("click", function() {
                $('#top-contact-us').find('.live-chat').trigger('click');
            });
            $(ref).find('.live-expert').off('click').on("click", function() {
                if ($('#gis-cta').length > 0) {
                    $('#gis-cta').trigger('click');
                    $(ref).find('.switch-button.activated').trigger('click');
                }
            });
            $(ref).find('.feed-back').on("click", function() {
                if ($('#kampyleButtonContainer button').length > 0) {
                    $('#kampyleButtonContainer button').trigger('click');
                }
                $(ref).find('.switch-button.activated').trigger('click');
            });
        };
        $('#right-contact-us').initContactUs({
            "callback": initPlugin
        });
        $('#top-contact-us').initContactUs({
            "fixedOn": "#contact-us-top",
            "callback": initPlugin
        });
    });
</script>
